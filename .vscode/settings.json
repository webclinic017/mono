{
    "files.exclude": {
        "android": true,
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "android": true,
    },
    "go.goroot": "~/development/mono/bazel-mono/external/go_sdk",
    "go.toolsEnvVars": {
      "GOPACKAGESDRIVER_BAZEL_QUERY": "kind(go_binary, //golang/...)",
      "GOPACKAGESDRIVER": "~/development/mono/scripts/bash/gopackagesdriver.sh"
    },
    "go.useLanguageServer": true,
}
